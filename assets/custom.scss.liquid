body{
  background-color:#F5F1ED;
}
.button .icon{
font-weight: 700;
}
#shopify-section-pxs-announcement-bar{
  .pxs-announcement-bar{
    font-weight: 600;
    font-size: 13px;
    line-height: 20px; 
    padding: 10px 25px;
  }
}
.announcement-bar{
  .icon-cross{
    font-size: 18px;
    color: #fff;
    position: absolute;
    right: 15px;
    top: 6px;
    z-index: 99;
    cursor: pointer;
  }
}
.header{
  .header-tools .navigation summary {
    display: block;
    a.icon {
      line-height: .5;
      position: relative;
      top: 2px;
      margin-left: 0;
    }
  }
  .main-header-wrapper{
    max-width: 1440px; 
    padding:0 45px;
    @media (max-width:991px){
      padding:0 30px;
    }
    @media (max-width:720px){
      padding:0 15px;
    }
  }
  .main-header{
    align-items: center;
    border: 0;
    padding:0 !important;
    @media (max-width:720px){
      height:auto;
      overflow: visible !important;
    }
    .branding{
      flex:1;
      .branding-logo-link{
        height:auto;
        padding: 27px 0;
        .branding-logo-heading{
          height:100%;
          display: flex;
          align-items: center;
        }
        @media (max-width:720px){
          display:flex;
        }
      }
    }
    .header-tools{
      margin-left:0;
      .header-navigation-list-item{
        padding: 0px 20px;
        .header-navigation-link{
          padding: 10px 0px;
          font-weight: 500;
          font-size: 18px;
          line-height: 27px;
          height:auto;
          a.icon {
            font-size: 20px;
          }
          &:after{
            bottom: 0px !important;
          }
          &:not(.secondary-link):hover:after{
            display:block;
            position: absolute;
            left: 0;
            z-index: -1;
            width: 100%;
            height: 1px;
            content: "";
          }
          &.header-navigation-current{
            font-weight: 600;
          }
        }
        .has-dropdown{
          .primary-link{
            &:after{
              display:none !important;
            }
          }
        }
      }
      .secondary-list {
        top: 45px;
        left: -80px;
        padding: 10px 0;
        border: 0;
        border-radius: 23px;
        overflow:hidden;
        .secondary-list-item {
          padding: 3px 0 !important;
          .secondary-link {
            text-align: center;
            font-weight: 500;
            font-size: 15px;
            line-height: 22px;
            color: #223B43;
            padding: 7px;
            &:hover{
              background-color: #F5F5F5;
            }
            &.secondary-navigation-current {
              background-color: #F5F5F5;
              font-weight: 700 !important;
            }
          }
        }
      }
    }
    .header-actions{
      flex:1;
      &:before{
        display:none;
      }
      .header-actions-list{
        justify-content: flex-end;
      }
      .header-cart-link {
        font-size: 26px;
        color: #223B43;
      }
      @media (max-width:720px){
        flex:0;
        .header-cart-link {
          position: absolute;
          width: auto;
          right: 10px;
          left: auto;
          .header-cart-count{
            top: 5px !important;
            right: -15px !important;
          }
        }
        .drawer-toggle {
          right: auto;
          left: 10px;
          width: auto;
          font-size: 26px;
          color: #223B43;
        }
      }
    }
  }
  &.scrolled,&:not(.higher-than-slideshow){
  background-color:#f5f1ede3 !important;
    @media (max-width:720px){
      position: fixed !important;
    }
    .branding-logo-link{
      color:#292929;
    }
    .header-tools .header-navigation-list-item .header-navigation-link{
    color:#223B43;
      &:after{
        background-color:#223B43;
      }
    }
  }
&.higher-than-slideshow{
  .branding-logo-link,.header-navigation-link{
    color:#ffffff;
    &:after{
        background-color:#fff;
      }
  }
  .header-cart-link,.drawer-toggle{
    color:#ffffff !important;
  }
}
  @media (max-width:720px){
    position: fixed !important;
    &.higher-than-slideshow{
      .branding-logo-link{
        color:#ffffff;
      }
      .header-cart-link,.drawer-toggle{
        color:#ffffff !important;
      }
    }
  }
}

body.no-announcement{
  padding-top: 0px !important;
  .header.sticky-header{
    top: 0px !important;
  }
  &:not(.has-slideshow-full-window){
    padding-top: 121px !important;
  }
}

.showing-drawer{
  .branding-logo-link,
  .header-cart-link,
  .header-cart-link {
    color:#fff !important;
  }
  .sticky-header{
    background-color: #1D333B !important;
  }
}
.footer-wrapper {
  background-color: #a3bfc9;
  padding-top: 20px !important;
  .main-footer{
    max-width: 1320px;
    .footer-menu-title {
      font-size: 22px;
      font-weight: 700;
      color:#ffffff;
      letter-spacing: 0px;
      text-transform: capitalize;
      @media (max-width:1100px){
        font-size: 18px;
      }
      @media (max-width:500px){
        font-size: 24px;
        text-align: center;
      }
    }
    @media (max-width:500px){
      .footer-row{
        padding-top: 20px;
        padding-bottom: 10px;
        display: flex;
        flex-direction: column;
        .footer-column{
          order:1;
          &.footer-menu{
            order:2;
          }
        }
      }
    }
    li.footer-menu-list-item {
      font-size: 15px;
      @media (max-width:1100px){
        font-size: 13px;
      }
      @media (max-width:500px){
        font-size: 16px;
        text-align: center;
      }
      .footer-menu-list-item-anchor {
        color: #ffffff;
        margin-top: 8px;
        &:hover{
          color:#CF812D;
        }
      }
      @media (min-width:501px) and (max-width:1050px){
        &:first-child .footer-menu-list-item-anchor {
          word-break: break-all;
        }
      }
    }
    .footer-information {
        display: flex;
        justify-content: space-between;
        width: 100%;
        align-items: flex-end;
    }
    .newsletter-column{
      width:50%;
      @media (max-width:500px){
        padding-left:0;
        padding-right:0;
      }
      .newsletter-form-container{
        max-width:400px;
        @media (max-width:500px){
          .newsletter-form-container{
            padding-left:0;
            padding-right:0;
          }
        }
        .title{
          font-size: 26px;
          font-weight: 500;
          color: #fff;
        }
        .subtitle{
          font-size: 13px;
          font-weight: 400;
          color: #fff;
          padding: 5px 0;
        }
      }
      input.newsletter-form-email {
        padding: 8px 120px 8px 30px !important;
        border-radius: 50px;
        line-height: 64px;
        height: 64px;
        font-weight: 600;
        &:focus{
          background-color:#f3eeed;
          outline:0;
        }
        @media (max-width:500px){
          padding: 10px 30px !important;
          text-align: center;
        }
      }
      .newsletter-form-cta {
        border-radius: 36px;
        height: auto;
        width: auto;
        display: inline-block;
        top: 12px;
        right: 12px;
        @media (max-width:500px){
          position:static;
          display: block;
          width: 100%;
        }
        input.newsletter-form-submit {
          font-size: 13px;
          font-weight: 700;
          padding: 10px 25px;
          display: inline-block;
          width: auto;
          height: auto;
          color: #fff;
          border-radius: 20px;
          background-color: #c27e34;
          outline: 0 !important;
          &:hover{
            background-color:#FFB63D;
          }
          @media (max-width:500px){
            position:static;
            display: block;
            width: 100%;
            margin-top: 20px;
          }
        }
      }
    }
  }
  .sub-footer {
    padding-bottom: 50px;
    padding-top:0;
    @media (max-width:500px){
      padding-top:0;
      padding-bottom: 30px;
      .footer-logo-bottom{
        display:none;
      }
    }
    .footer-copyright{
      color:#fff;
      font-size:13px;
      width: calc(100% - 50px);
      text-align: left;
      @media (max-width:500px){
        width: 100%;
        text-align: center;
      }
    }
  }
  .footer-logo-link .branding-logo{
    color:#fff;
    @media (max-width:500px) {
      width: 248px;
      margin: 0 auto;
    }
  }
  .branding-logo svg {
    width: 185px;
}
  .footer-logo{
    display:none;
    @media (max-width:500px){
      display:block;
      text-align: center;
      svg{
        width:218px;
        max-width:70%;
        height: auto;
      }
    }
  }
  @media (max-width:920px){
    .footer-menu{
     width:33.33% !important; 
    }
    .newsletter-column {
      width: 100% !important;
      text-align: center;
    }
  }
  @media (max-width:500px){
    .footer-menu{
     width:100% !important; 
    }
  }
}



.slide-text{
  padding: 0 !important;
  max-width:750px;
  .slide-text-heading{
    font-weight: 700;
    font-size: 49px;
    line-height: 1.45;
    margin: -2px;
    @media (max-width:1199px){
      font-size: 46px;
    }
    @media (max-width:720px){
       font-size: 33px;
       line-height: 45px;
      span{
        font-size: 21px;
        display: block;
        position: relative;
        padding-bottom: 30px;
        padding-top: 0px;
        &:after{
          content: "";
          width: 60px;
          height: 2px;
          position: absolute;
          left: 0;
          bottom: 8px;
          background: #223B43;
        }
      }
    }
  }
  .slide-text-subheading{
    font-weight: 700;
    font-size: 21px;
    line-height: 1.45;
    @media (max-width:1199px){
      font-size: 19px;
    }
    @media (max-width:720px){
      font-size: 17px;
      width: 100%;
      max-width:305px;
    }
  }
  .button{
    background: #FFB63D;
    border: 4px solid #FFB63D;
    border-radius: 100px;
    font-weight: 700;
    font-size: 16px;
    line-height: 24px;
    text-align: center;
    letter-spacing: 0.21em;
    color: #fff;
    padding:12px 30px;
    min-width: 210px;
    margin-top: 40px;
    margin-right: 10px;
    @media (max-width:720px){
      margin-top: 20px;
    }
    &.button2 {
      background: transparent;
      border-color: #fff;
      margin-right: 0px;
      @media (max-width:720px){
        color: #0b2931;
        border: 4px solid #0b2931;
      }
      &:hover{
        background: #c27e34;
        border-color: #c27e34;
        color: #fff;
      }
    }
  }
  .app-btn{
    margin-top:40px;
    display: inline-block;
  }
}
.text-aligned-left .slide-text{
  transform: translate(0, -56%);
    padding-left: 0;
    left: 100px;
  @media (max-width:1199px){
    left: 60px;
  }
  @media (max-width:720px){
    padding:0 35px !important;
    left: 0px;
    text-align: left;
  }
}
.text-aligned-right .slide-text {
    transform: translate(0, -50%);
    padding-right: 0;
    right: 100px;
    left: auto;
  @media (max-width:1199px){
    right: 60px;
  }
  @media (max-width:720px){
    padding:0 35px !important;
    right: 0px;
    text-align: right;
  }
}
.first-banner{
  .text-wrap {
    position: absolute;
    left: 50%;
    top: 0;
    transform: translate(-50%);
    width: 100%;
    max-width: 1400px;
    height: 100%;
    z-index: 1;
  }
  .slide-image-container:after{
    content:'';
    width: 100vw;
    position: absolute;
    bottom: 0;
    left: 0;
    background-image: url(https://cdn.shopify.com/s/files/1/0618/6015/1453/files/curve.png?v=1659695723);
    padding: 2.2%;
    background-size: contain;
    background-repeat: no-repeat;
    background-position: bottom;
  }
  @media (max-width:720px){
    .slide-image-container:before {
      content: '';
      position: absolute;
      width: 100%;
      height: 40%;
      background: linear-gradient(0deg, #fff, transparent);
      bottom: 0;
    }
  }
   @media (max-width:720px){
    height: auto !important;
    overflow: visible;
    .slide-image-container{
      height: 65vh !important;
      min-height: 450px !important;
    }
    .text-wrap{
      position: static;
      transform: none;
      height: auto;
      .slide-text{
        position: static;
        transform: translateY(-50px);
        .slide-text-heading{
          color:#0B2931;
        }
        .slide-text-subheading{
          font-weight: 600;
          color: #474747;
        }
      }
    }
  } 
}
.banner-slide{
  .overlay:after{
    display:none;
  }
  .swiper-slide{
    background-color: #f5f1ed;
    background-image: none !important;
    @media (min-width:721px){
      height: 100% !important;
    }
  }
}

.cc-slide{
  min-height: 720px;
  .dynamic-slide{
    min-height: 720px;
  }
}
@media (min-width:721px){
.static-slideshow-wrapper.full-window:not(.video_banner) { 
    height: 90vh;
  .swiper-wrapper{
    max-height:90vh !important;
  }
}
}


.blog-posts{
  .dynamic-blog{
    padding:60px 0 10px;
    max-width: 1290px;
    .dynamic-blog-header{
      padding-bottom:50px;
      .dynamic-blog-title{
        font-weight: 700;
        font-size: 38px;
        color: #223B43;
        line-height: 1.45;
        @media (max-width:720px){
          font-size: 28px;
        }
      }
      .dynamic-blog-subtitle{
        font-size: 12px;
        line-height: 1.45;
        letter-spacing: 0.29em;
        color: #D97F27;
        font-weight: 600;
        display:none;
        @media (max-width:720px){
          display:block;
          margin-bottom: 16px;
        }
      }
    }
    .list {
      display:flex;
      flex-flow: wrap;
    }
    .list-item-inline{
      padding-top:0; 
      padding-bottom:0; 
      margin:0;
      @media (max-width:720px){
        margin-bottom: 30px;
        width:100%;
      }
    }
    a.list-container {
      position: relative;
      border-radius: 65px;
      box-shadow: 0px 30px 70px rgba(0, 0, 0, 0.06);
      background-color: #fff;
      height:100%;
      @media (max-width:720px){
        border-radius: 43px;
      }
      .list-cover {
        position: relative;
        padding-bottom: 65.472%;
        img {
          position: absolute;
          width: 100%;
          height: 100%;
          object-fit: cover;
          left: 0;
        }
      }
      .list-text-wrapper {
        padding: 45px 15px 30px !important;
        .list-title{
          font-weight: 700;
          font-size: 21px;
          line-height: 1.45;
          color: #223B43;
          max-width: 280px;
          margin: 0 auto 15px;
          @media (max-width:1199px){
            font-size: 19px;
          }
        }
      }
      .dynamic-blog-cta {
        display: block !important;
        font-size: 20px;
        position: absolute;
        left: 50%;
        top: 0;
        transform: translate(-50%,-50%);
        margin: 0;
        background: #FFB63D;
        border-radius: 100px;
        padding: 10px 25px;
        &:hover {
          background-color: #cf7e2d;
          color: #fff;
          border-color: #cf7e2d;
        }
        @media (max-width:720px){
          padding: 8px 20px;
        }
        .list-link:after{
          display:none;
        }
        .list-link {
          position: static;
          display: block;
          line-height: 1.45;
          color: #fff;
          font-weight: 700;
          font-size: 13px;
          position:relative;
          top:1px;
          @media (max-width:720px){
            font-size: 11px;
          }
          .icon{
            font-size: 20px;
            line-height: 0;
            position: relative;
            top: 3px;
            left: 2px;
            font-weight: bold;
            @media (max-width:720px){
              font-size: 14px;
              top: 2px;
            }
          }
        }
      }
      .author {
        display: flex;
        align-items: center;
        justify-content: center;
        .author-image {
          display: flex;
          position: relative;
          color:#fff;
          margin-right:10px;
          z-index: 1;
          img{
            position: absolute;
            left: 0;
            top: 0;
            z-index: -1;
            height: 100%;
            object-fit: cover;
            object-position: center;
          }
          svg{
            width: 25px;
          }
        }
        p.author_name {
          font-weight: 700;
          font-size: 13px;
          line-height: 1.45;
          color: #223B43;
          padding: 0;
          span{
            font-weight: 500;
          }
        }
      }
      span.tag {
        position: absolute;
        background: red;
        z-index: 1;
        left: 30px;
        top: 30px;
        border-radius: 11px;
        font-size: 12px;
        font-weight: 700;
        letter-spacing: 0.1em;
        color: #fff;
        display: block;
        padding: 9px 20px;
        line-height: 1.45;
        text-transform: uppercase;
        @media (max-width:720px){
          font-size: 11px;
          padding: 7px 14px;
          top: 20px;
          left: 20px;
        }
      }
    }
    .dynamic-blog-button {
      width: 250px;
      font-size: 17px;
      line-height: 26px;
      text-align: center;
      letter-spacing: 0.16em;
      color: #223B43;
      border: 2px solid #223B43;
      border-radius: 50px;
      padding: 15px;
      font-weight: 700;
      &:hover{
          background-color: #cf7e2d;
          color: #fff;
          border-color: #cf7e2d;
      }
      .icon{
        font-size: 24px;
        font-weight: bold;
      }
    }
  }
}


.faq{
  .container{
    padding: 45px 30px;
    width:980px !important;
    @media (max-width:720px){
      padding: 20px 0px;
    }
    .title{
      h2{
        font-weight: 700;
        font-size: 36px;
        line-height: 1.45;
        color: #0B2931;
        margin-bottom: 50px;
        @media (max-width:720px){
          font-size: 25px;
          max-width: 250px;
          margin-left: auto;
          margin-right: auto;
          margin-bottom: 40px;
        }
      }
    }
    .wrapper{
      text-align:left;
      .question{
        background: #ffffff;
        margin-bottom: 15px;
        padding: 20px 25px;
        border-radius: 37px;
        @media (max-width:720px){
          padding: 10px 10px;
          border-radius: 28px;
        }
      }
      .faq-heading{
        font-weight: 700;
        font-size: 22px;
        line-height: 1.45;
        color: #223B43;
        padding: 20px;
        cursor: pointer;
        @media (max-width:720px){
          font-size: 13px;
          color: #1D333B;
        }
        @media (max-width:720px){
          padding: 15px;
        }
        .icon{
          color: #a7c6d0;
          font-size: 30px;
          line-height: 0;
          position: relative;
          top: 6px;
          font-weight: 700;
          display: inline-block;
          transition: .3s;
          @media (max-width:720px){
            font-size: 25px;
          }
        }
        &.active .icon{
          transform: rotate(-180deg);
        }
        .text{
          padding-left: 5px;
        }
      }
      .faq-text-content{
        padding:0px 20px;
        p {
          padding:0px 0px 12px;
          color: #474747;
          font-weight: 500;
        }
        @media (max-width:720px){
              font-size: 13px;
        }
      }
    }
  }
}


.review{
  padding:40px 0;
  overflow:hidden;
  .module-wrapper {
    width: 100%;
    max-width: 1440px;
    margin: auto;
    .bg-slider{
      max-width: 1200px;
      margin: auto;
      @media (max-width:450px){
        max-width:98%;
      }
    }
    .reviwe-image{
/*       @media (max-width:720px){
        img{
          transform: scale(1.3);
          transform-origin: top;
        }
      } */
