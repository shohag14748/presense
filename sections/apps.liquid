<section class="product-app--container">
  <div class="module-wrapper">
    <div class="module app-wrapper">
      {% for block in section.blocks %}
        {% render block %}
      {% endfor %}
    </div>
  </div>
</section>

{% schema %}
  {
    "name": "App wrapper",
    "blocks": [
      {
        "type": "@app"
      }
    ],
    "presets": [
      {
        "name": "App wrapper"
      }
    ]
  }
{% endschema %}
